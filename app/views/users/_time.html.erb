<td><%= shift.time1 %></td>
<td><%= shift.time2 %></td>
<td><%= shift.time3 %></td>
<td><%= shift.time4 %></td>
<td><%= shift.time5 %></td>
<td><%= shift.time6 %></td>
<td><%= shift.time7 %></td>
<% if shift.user == current_user %>
  <td>
    <%= link_to '訂正', edit_job_path(shift.id) %>
  </td>
  <td>
    <%= link_to '削除', job_path(shift.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>
  </td>
<% end %>